<?xml version="1.0" ?>

<odoo>
    <data>
		<!--Hotel wizard form view  -->
		<record model="ir.ui.view" id="arrival_dept_guest_wizard_view">
			<field name="name">arrival.dept.guest.wizard.form</field>
			<field name="model">arrival.dept.guest.wizard</field>
			<field name="arch" type="xml">
				<form string="Report Wizard">
					<header>
                    	<button special="cancel" string="Cancel" icon="fa-times"/>
						<button name="print_report" string="Print Report" type="object" icon="fa-backward"/>
                   </header>
					<separator string="Fill The Details" colspan="4"/>
					<group colspan="4" col="4">
						<field name="date_start"/>
						<field name="arrival_dept"  />
					</group>
					 <footer>
                    <button name="your_test_method" string="My Button" type="object" class="btn-primary" invisible = "1"/> 
                    <button string="Cancel" class="btn-default" special="cancel" invisible = "1"/>   
                </footer>
				</form>
			</field>
		</record>

	
		
		
	</data>
</odoo>